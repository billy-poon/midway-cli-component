{
  "name": "midway-cli-component",
  "version": "0.0.4",
  "description": "This is a midway component(Framework) running in CLI",
  "main": "dist/index.js",
  "typings": "index.d.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=local tsx bootstrap.js",
    "build": "cross-env mwts check && midway-bin build -c",
    "test": "cross-env midway-bin test --ts",
    "cov": "cross-env midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix"
  },
  "license": "MIT",
  "devDependencies": {
    "@midwayjs/cli": "^2.0.0",
    "@midwayjs/core": "^3.13.0",
    "@midwayjs/decorator": "^3.13.0",
    "@midwayjs/logger": "^3.1.2",
    "@midwayjs/mock": "^3.13.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^16.11.22",
    "@types/yargs": "^17.0.32",
    "cross-env": "^6.0.0",
    "jest": "^29.2.2",
    "mwts": "^1.3.0",
    "ts-jest": "^29.0.3",
    "tsx": "^4.6.2",
    "typescript": "~4.8.0"
  },
  "optionalDependencies": {
    "@midwayjs/cli-plugin-faas": "^2.1.2",
    "@midwayjs/cli-plugin-generator": "^2.1.0"
  },
  "dependencies": {
    "@midwayjs/bootstrap": "^3.13.0",
    "yargs": "^17.7.2"
  },
  "author": "Billy Poon",
  "repository": {
    "type": "git",
    "url": "https://github.com/billy-poon/midway-cli-component"
  },
  "keywords": [
    "midway",
    "cli",
    "yargs",
    "typescript",
    "decorator",
    "command"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "index.d.ts"
  ]
}
